<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <AssemblyName>CakeStore.Order.Service.Server</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>CakeStore.Order.Service.Server</PackageId>
    <PackageTargetFallback>$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Interactive.Async" Version="3.0.0" />
    <PackageReference Include="Google.ProtoBuf" Version="3.1.0" />
    <PackageReference Include="Grpc.Core" Version="1.0.1-pre1" />
    <PackageReference Include="Grpc" Version="1.0.1-pre1" />
  </ItemGroup>

</Project>
