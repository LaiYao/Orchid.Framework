{
    "authors": [
        "https://github.com/LaiYao/Orchid"
    ],
    "version": "1.0.0-*",
    "description": "Orchid.Repo Class Library",
    "packOptions": {
        "repository": {
            "type": "git",
            "url": "git://github.com/LaiYao/Orchid.Framework"
        }
    },
    "buildOptions": {
        "warningsAsErrors": true,
        "keyFile": "../../tools/Key.snk",
        "nowarn": [
            "CS1591"
        ],
        "xmlDoc": true
    },
    "dependencies": {
        "Microsoft.EntityFrameworkCore":"1.0.0-*",
        "Orchid.Core": "1.0.0-*",
        "Orchid.Repo":"1.0.0-*"
    },
    "frameworks": {
        "netstandard1.0": {
            "dependencies": {
                "System.ComponentModel": "4.0.1-*",
                "System.Diagnostics.Debug": "4.0.11-*",
                "System.Globalization": "4.0.11-*",
                "System.Linq": "4.1.0-*",
                "System.Linq.Expressions": "4.1.0-*",
                "System.Resources.ResourceManager": "4.0.1-*"
            }
        }
    }
}
